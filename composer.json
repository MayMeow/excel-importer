{
    "name": "maymeow/excel-importer",
    "type": "php-pacakge",
    "description": "A php package to transform XLSX filse to models.",
    "keywords": ["php", "package", "utils", "excel", "xlsx-to-model", "datasources"],
    "homepage": "https://github.com/MayMeow/excel-importer",
    "authors": [{
        "name": "May Meow",
        "email": "excel-importer.9gfpk@maymeow.mozmail.com",
        "homepage": "https://themaymeow.com"
    }],
    "license": "MIT",
    "require": {
        "phpoffice/phpspreadsheet": "^1.13",
        "symfony/console": "^5.1",
        "php": "8.*",
        "meow/hydrator": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MayMeow\\ExcelImporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MayMeow\\ExcelImporter\\Test\\": "tests/"
        }
    },
    "support": {
        "source": "https://github.com/MayMeow/excel-importer",
        "issues": "https://github.com/MayMeow/excel-importer/issues"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.1",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "test": "phpunit tests",
        "cs-check": "phpcs --standard=PSR2 src",
        "cs-fix": "phpcbf --standard=PSR2 src",
        "stan": "phpstan analyse"
    }
}
